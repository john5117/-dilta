  "compilerOptions": {
    "outDir": "../../dist/out-tsc/apps/installation",
    "module": "es2015",
    "baseUrl": ".",
    "types": [],
    "paths": {
      "@dilta/process": [
        "../../libs/process"
      ],
      "@dilta/store": [
        "../../libs/store"
      ],
      "@dilta/commonwebui": [
        "../../libs/commonwebui"
      ],
      "@dilta/models": [
        "../../libs/models"
      ],
      "@dilta/util": [
