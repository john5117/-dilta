{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@dilta/process": [
        "libs/process"
      ],
      "@dilta/store": [
        "libs/store"
      ],
      "@dilta/commonwebui": [
        "libs/commonwebui"
      ],
      "@dilta/models": [
        "libs/models"
      ],
      "@dilta/util": [
        "libs/util"
      ],
      "@dilta/screwbox": [
        "libs/screwbox"
      ],
      "@dilta/presets": [
        "libs/presets"
      ],
      "@dilta/offlinedatabase": [
        "libs/offlinedatabase"
      ],
      "@dilta/*": [
        "libs/*"
      ]
    },
    "baseUrl": "."
  },
  "include": [
    "main.ts"
  ]
}
