{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@dilta/process": [
        "libs/process"
      ],
      "@dilta/store": [
        "libs/store"
      ],
      "@dilta/commonwebui": [
        "libs/commonwebui"
      ],
      "@dilta/models": [
        "libs/models"
      ],
      "@dilta/util": [
        "libs/util"
      ],
      "@dilta/screwbox": [
        "libs/screwbox"
      ],
      "@dilta/presets": [
        "libs/presets"
      ],
      "@dilta/offlinedatabase": [
        "libs/offlinedatabase"
      ],
      "@dilta/security": [
        "libs/security"
      ],
      "@dilta/electron": [
        "libs/electron"
      ],
      "@dilta/abstract-imp": [
        "libs/abstract-imp"
      ],
      "@dilta/*": [
        "libs/*"
      ],
      "@dilta/auth-module": [
        "libs/auth-module/src/index.ts"
      ],
      "busar/*": [
        "dist/busar/*"
      ],
      "@@dilta/busar": [
        "libs/busar/src/index.ts"
      ]
    },
    "baseUrl": "."
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts",
    "src/test.ts"
  ]
}
