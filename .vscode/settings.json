{
  "projectManager.git.ignoredFolders": [
    "node_modules",
    "out",
    "typings",
    "test",
    ".haxelib",
    "dist"
  ],
  "cSpell.words": [
    "dilta"
  ],
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.tsdk": "node_modules\\typescript\\lib",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.*.spec.ts": true
  }
}
