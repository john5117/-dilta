{
  "projectManager.git.ignoredFolders": [
    "node_modules",
    "out",
    "typings",
    "test",
    ".haxelib",
    "dist"
  ],
  "cSpell.words": [
    "dilta"
  ],
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.tsdk": "node_modules\\typescript\\lib",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.*.spec.ts": true,
    "apps/demo": true,
    "apps/*-e2e": true,
    "coverage": true,
    "doc": true,
    "dist": false,
    "keys": true,
    "node_modules": true,
    "tools": true,
    "typings": true,
    "apps": false,
    "assets": true,
    "backup_src": true,
    ".vscode": true,
    "libs/abstract-imp": true,
    "libs/auth-module": false,
    "libs/electron": false,
    "libs/generator": true,
    "libs/models": false,
    "libs/offlinedatabase": false,
    "libs/presets": true,
    "libs/process": true,
    "libs/screwbox": false,
    "libs/security": true,
    "libs/store": false,
    "libs/util": false,
    "libs/common-ui": false,
    "libs/commonwebui": false,
    "libs/busar": false,
    "libs/busar-base": false,
  },
  "typescript.format.insertSpaceAfterTypeAssertion": true,
  "typescript.preferences.quoteStyle": "single",
  "typescript.preferences.importModuleSpecifier": "auto",
  "line-counter.excludes": [
    "**/.vscode/**",
    "**/libs/presets/src/lib/nigeria.states.ts"
  ],
  "line-counter.includes": [
    "**/apps/**",
    "**/libs/**",
    "main.ts"
  ]
}
